--3. What about the busiest?
SELECT
	sfo_calendar.calender_date,
	(sfo_calendar.available='false') as min_availability_calendar,
	MIN(sfo_listings.availability_365) as min_availability_listings

FROM sfo_calendar

JOIN sfo_reviews
ON sfo_calendar.listing_id=sfo_reviews.listing_id

JOIN sfo_listings
ON sfo_listings.id=sfo_reviews.id

GROUP BY 1,2
ORDER BY min_availability_listings
LIMIT 10;